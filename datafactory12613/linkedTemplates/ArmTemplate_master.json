{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory12613"
		},
		"AzureSynapseAnalytics1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSynapseAnalytics1'"
		},
		"LS_ADLS_NYCPayroll_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS_NYCPayroll'"
		},
		"LS_SQL_NYCPayroll2021_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQL_NYCPayroll2021'"
		},
		"LS_Syn_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_Syn'"
		},
		"AzureSynapseAnalytics1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "my-synapse-ondemand.sql.azuresynapse.net"
		},
		"AzureSynapseAnalytics1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "udacity"
		},
		"AzureSynapseAnalytics1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Azure-rashed"
		},
		"LS_ADLS_NYCPayroll_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageacc27810.dfs.core.windows.net/"
		},
		"LS_Syn_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://synnndatalake.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory12613_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSynapseAnalytics1_password": {
						"value": "[parameters('AzureSynapseAnalytics1_password')]"
					},
					"LS_ADLS_NYCPayroll_accountKey": {
						"value": "[parameters('LS_ADLS_NYCPayroll_accountKey')]"
					},
					"LS_SQL_NYCPayroll2021_connectionString": {
						"value": "[parameters('LS_SQL_NYCPayroll2021_connectionString')]"
					},
					"LS_Syn_accountKey": {
						"value": "[parameters('LS_Syn_accountKey')]"
					},
					"AzureSynapseAnalytics1_properties_typeProperties_server": {
						"value": "[parameters('AzureSynapseAnalytics1_properties_typeProperties_server')]"
					},
					"AzureSynapseAnalytics1_properties_typeProperties_database": {
						"value": "[parameters('AzureSynapseAnalytics1_properties_typeProperties_database')]"
					},
					"AzureSynapseAnalytics1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSynapseAnalytics1_properties_typeProperties_userName')]"
					},
					"LS_ADLS_NYCPayroll_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_NYCPayroll_properties_typeProperties_url')]"
					},
					"LS_Syn_properties_typeProperties_url": {
						"value": "[parameters('LS_Syn_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "datafactory12613_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'datafactory12613_ArmTemplate_0')]"
			]
		}
	]
}